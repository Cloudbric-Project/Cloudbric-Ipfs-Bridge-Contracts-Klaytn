       whiteList = WhiteList(whiteListContractAddress);\n    }\n\n    /**\n    * @dev get length of Ipfs CIds of black ip data which is detected by Cloudbric WAF.\n    */\n    function wafBlackIpListSize()\n        public\n        view\n        returns (uint)\n    {\n        return wbiLookUpTable.length;\n    }\n\n    /**\n    * @dev get Ipfs CId of black ip data detected by Cloudbric WAF using index.\n    * @notice you should know size of lookup table.\n    */\n    function getWafBlackIpAtIndex(uint _index)\n        public\n        view\n        returns (bytes32 hash, uint8 hash_funciton, uint8 size)\n    {\n        bytes32 wafBlackIpIdx = wbiLookUpTable[_index];\n        require(\n            _index < wbiLookUpTable.length,\n            \"Given index is out of bound.\"\n        );\n\n        Multihash storage multihash = wafBlackIpList[wafBlackIpIdx];\n        return (multihash.hash, multihash.hashFunction, multihash.size);\n    }\n\n    /**\n    * @dev get Ipfs CId of black ip data detected by Cloudbric WAF using Cloudbric Data Index.\n    * @param _clbIndex Cloudbric Index which is used by Cloudbric Labs or Cloudbric Database.\n    */\n    function getWafBlackIpAtClbIndex(bytes32 _clbIndex)\n        public\n        view\n        returns (bytes32 hash, uint8 hash_funciton, uint8 size)\n    {\n        Multihash storage multihash = wafBlackIpList[_clbIndex];\n        return (multihash.hash, multihash.hashFunction, multihash.size);\n    }\n\n    /**\n    * @dev add Ipfs CId of black ip data which is detected by Cloudbric WAF.\n    * @param _clbIndex The Index of Cloudbric ThreatDB.\n    * @param _hash The Ipfs hash which came from mutlihash identifier.\n    */\n    function addWafBlackIp(\n        bytes32 _clbIndex,\n        bytes32 _hash,\n        uint8 _hashFunction,\n        uint8 _size\n    )\n        public\n        onlyWhiteListed(msg.sender)\n        returns (bool)\n    {\n        wbiLookUpTable.push(_clbIndex);\n        wafBlackIpList[_clbIndex] = Multihash(_hash, _hashFunction, _size);\n        emit AddWafBlackIp(msg.sender, _clbIndex, _hash, _hashFunction, _size);\n        return true;\n    }\n}","sourcePath":"/Users/aeharv/Dropbox/Cloudbric/projects/public_github/Cloudbric-Ipfs-Bridge-Contracts-Klaytn/contracts/CloudbricWafBlackIpStorage.sol","ast":{"absolutePath":"/Users/aeharv/Dropbox/Cloudbric/projects/public_github/Cloudbric-Ipfs-Bridge-Contracts-Klaytn/contracts/CloudbricWafBlackIpStorage.sol","exportedSymbols":{"CloudbricWafBlackIpStorage":[382]},"id":383,"nodeType":"SourceUnit","nodes":[{"id":203,"literals":["solidity","0.4",".24"],"nodeType":"PragmaDirective","src":"0:23:1"},{"absolutePath":"/Users/aeharv/Dropbox/Cloudbric/projects/public_github/Cloudbric-Ipfs-Bridge-Contracts-Klaytn/contracts/open-zeppelin/Ownable.sol","file":"./open-zeppelin/Ownable.sol","id":204,"nodeType":"ImportDirective","scope":383,"sourceUnit":635,"src":"25:37:1","symbolAliases":[],"unitAlias":""},{"absolutePath":"/Users/aeharv/Dropbox/Cloudbric/projects/public_github/Cloudbric-Ipfs-Bridge-Contracts-Klaytn/contracts/WhiteList.sol","file":"./WhiteList.sol","id":205,"nodeType":"ImportDirective","scope":383,"sourceUnit":547,"src":"63:25:1","symbolAliases":[],"unitAlias":""},{"baseContracts":[{"arguments":null,"baseName":{"contractScope":null,"id":206,"name":"Ownable","nodeType":"UserDefinedTypeName","referencedDeclaration":634,"src":"295:7:1","typeDescriptions":{"typeIdentifier":"t_contract$_Ownable_$634","typeString":"contract Ownable"}},"id":207,"nodeType":"InheritanceSpecifier","src":"295:7:1"}],"contractDependencies":[634],"contractKind":"contract","documentation":"@title CloudbricWafBlackIpStorage\n@dev The CloudbricIpfsBridge contract has an Ipfs CId(Contents Identifier) of\nblack ips detected by Cloudbric WAF.","fullyImplemented":true,"id":382,"linearizedBaseContracts":[382,634],"name":"CloudbricWafBlackIpStorage","nodeType":"ContractDefinition","nodes":[{"canonicalName":"CloudbricWafBlackIpStorage.Multihash","id":214,"members":[